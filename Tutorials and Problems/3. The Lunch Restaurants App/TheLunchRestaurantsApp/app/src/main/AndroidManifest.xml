<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pt.up.feup.dn.thelunchrestaurantsapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/rest_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rest_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.TheLunchRestaurantsApp">
        <activity
            android:name="pt.up.feup.dn.thelunchrestaurantsapp.DetailsActivity"
            android:exported="false" />
        <activity
            android:name="pt.up.feup.dn.thelunchrestaurantsapp.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>